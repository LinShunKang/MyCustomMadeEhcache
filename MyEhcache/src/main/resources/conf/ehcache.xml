<config
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xmlns='http://www.ehcache.org/v3'
        xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.3.xsd">

    <!--<default-serializers>-->
        <!--<serializer type="ehcache.BZPCompressSerializer"/>-->
    <!--</default-serializers>-->

    <!--<default-copiers>-->
        <!--<copier type="ehcache.BZPCopier"/>-->
    <!--</default-copiers>-->

    <cache-template name="defaultListCacheTemplate">
        <key-type>java.lang.Long</key-type>
        <value-type>java.util.List</value-type>
        <resources>
            <heap unit="entries">10</heap>
            <offheap unit="MB">1</offheap>
        </resources>
        <expiry>
            <ttl unit="minutes">5</ttl>
        </expiry>
        <heap-store-settings>
            <max-object-graph-size>1000</max-object-graph-size>
            <max-object-size>10240</max-object-size>
        </heap-store-settings>
    </cache-template>

    <cache alias="simpleListCache" uses-template="defaultListCacheTemplate"/>

</config>